{
  "name": "laravel-doctrine/acl",
  "description": "Doctrine ACL for Doctrine 2 and Laravel 5",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "laravel",
    "orm",
    "data mapper",
    "database",
    "acl",
    "abilities",
    "policies",
    "permissions",
    "roles",
    "organisations"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/auth": "5.1.*|5.2.*",
    "illuminate/config": "5.1.*|5.2.*",
    "illuminate/contracts": "5.1.*|5.2.*",
    "illuminate/support": "5.1.*|5.2.*",
    "laravel-doctrine/orm": "1.0.*|1.1.*|1.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "LaravelDoctrine\\ACL\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  }
}
